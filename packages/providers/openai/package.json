{
  "name": "@sentinel/provider-openai",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:js": "tsup --config tsup.config.ts",
    "build:types": "tsc -p tsconfig.types.json --emitDeclarationOnly",
    "build": "pnpm clean && pnpm build:js && pnpm build:types",
    "dev": "tsup --config tsup.config.ts --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@sentinel/core": "workspace:*",
    "@sentinel/provider-types": "workspace:*",
    "openai": "^5.19.1"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
